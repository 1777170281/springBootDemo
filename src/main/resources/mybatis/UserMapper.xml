<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tttare.springDemo.userCenter.dao.UserMapper">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="User" id="userMap">

        <id column="id" property="userId" jdbcType="VARCHAR" />
        <result column="user_name" property="userName" jdbcType="VARCHAR" />
        <result column="password" property="password" jdbcType="VARCHAR" />
        <result column="create_date" property="createDate" />
        <result column="update_date" property="updateDate" />
        <result column="type" property="type" jdbcType="INTEGER" />
        <result column="is_ban" property="isBan" jdbcType="INTEGER" />
    </resultMap>



    <select id="listAllUser"  resultMap="userMap">
        select id,user_name,create_date,update_date,type,is_ban from tb_user
    </select>



</mapper>